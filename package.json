{
  "name": "fether-turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "dotenv -- turbo run dev --parallel",
    "lint": "turbo run lint",
    "prisma:generate": "turbo run prisma:generate",
    "prisma:push": "turbo run prisma:push",
    "prisma:push-accept-data-loss": "turbo run prisma:push-accept-data-loss",
    "prisma:migrate-reset": "turbo run prisma:migrate-reset",
    "studio": "turbo run prisma:studio",
    "prisma:seed": "turbo run prisma:seed",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "anvil": "anvil --block-time 5 --chain-id 696969 --steps-tracing",
    "ngrok": "ngrok http --domain=fether-testing.ngrok.app 3001",
    "startup": "open -a iTerm.app ./startup.zsh"
  },
  "dependencies": {
    "@manypkg/cli": "^0.19.2",
    "dotenv-cli": "^7.2.1",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@7.14.1"
}
