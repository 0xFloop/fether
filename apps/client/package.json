{
  "name": "fether-frontend",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && npm run build:remix",
    "build:css": "tailwindcss -i ./styles/app.css -o ./app/styles/app.css -m",
    "build:remix": "remix build",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:remix\"",
    "dev:css": "tailwindcss -i ./styles/app.css -o ./app/styles/app.css -w",
    "dev:remix": "pnpm with-env remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "start": "remix-serve build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@remix-run/node": "^1.14.1",
    "@remix-run/react": "^1.14.1",
    "@remix-run/vercel": "^1.14.1",
    "@vercel/node": "^2.6.2",
    "isbot": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-typical": "^0.1.3",
    "typewriter-effect": "^2.19.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.14.1",
    "@remix-run/eslint-config": "^1.14.1",
    "@remix-run/serve": "^1.14.1",
    "@tailwindcss/forms": "^0.5.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-typical": "^0.1.0",
    "eslint": "^8.27.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
