{
  "name": "fether-frontend",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "build:css": "tailwindcss -i ./styles/app.css -o ./app/styles/app.css -m",
    "build:remix": "remix build",
    "dev": "PORT=3000 concurrently \"npm run dev:css\" \"npm run dev:remix\"",
    "dev:css": "tailwindcss -i ./styles/app.css -o ./app/styles/app.css -w",
    "dev:remix": "pnpm remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "start": "remix-serve build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@rainbow-me/rainbowkit": "1.0.1",
    "@remix-run/node": "^1.8.0",
    "@remix-run/react": "^1.8.0",
    "@remix-run/server-runtime": "^1.17.1",
    "@remix-run/vercel": "^1.8.0",
    "@vanilla-extract/sprinkles": "^1.6.0",
    "@vercel/remix": "^1.17.0",
    "abitype": "^0.7.1",
    "database": "workspace:*",
    "isbot": "latest",
    "lucide-react": "^0.244.0",
    "octokit": "^2.0.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typewriter-effect": "^2.19.0",
    "viem": "1.1.2",
    "wagmi": "1.2.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.14.1",
    "@remix-run/eslint-config": "^1.14.1",
    "@remix-run/serve": "^1.14.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tanstack/query-core": "^4.29.14",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-typical": "^0.1.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.27.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
